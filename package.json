{
    "name": "master-application",
    "version": "1.0.0",
    "description": "Nest TypeScript starter repository",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "start": "nest start",
        "dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/apps/gateway/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./apps/gateway/test/jest-e2e.json",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm:generate": "npm run typeorm -- migration:generate -d ./src/config/DataSource.ts src/database/migrations/",
        "migration:create": "cd ./src/database/migrations && typeorm-ts-node-commonjs migration:create",
        "entity:create": "cd ./src/entities && typeorm-ts-node-commonjs entity:create",
        "seed": "npx tsx ./src/database/seeders/config.ts",
        "migration:refresh": "npx tsx src/database/RefreshMigrations.ts",
        "migration:run": "npx tsx src/database/RunMigrations.ts",
        "migration:run:seed": "npx tsx src/database/RunMigrations.ts --seed",
        "migration:refresh:seed": "npx tsx src/database/RefreshMigrations.ts --seed",
        "subscriber:create": "cd ./src/subscribers && typeorm subscriber:create"
    },
    "dependencies": {
        "@apollo/gateway": "2.10.0",
        "@apollo/server": "4.11.3",
        "@apollo/subgraph": "2.10.0",
        "@as-integrations/fastify": "^2.1.1",
        "@fastify/multipart": "^9.0.3",
        "@fastify/static": "^8.0.0",
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/apollo": "13.0.3",
        "@nestjs/common": "11.0.10",
        "@nestjs/core": "11.0.10",
        "@nestjs/graphql": "13.0.3",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "11.0.10",
        "@nestjs/platform-fastify": "^11.0.10",
        "@nestjs/typeorm": "^11.0.0",
        "argon2": "^0.41.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.7",
        "file-type": "^20.4.1",
        "graphql": "16.10.0",
        "graphql-query-complexity": "^1.0.0",
        "graphql-tools": "9.0.13",
        "graphql-type-json": "^0.3.2",
        "handlebars": "^4.7.8",
        "luxon": "^3.5.0",
        "mjml": "^4.15.3",
        "mjml-table": "^4.15.3",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.12.0",
        "nestjs-paginate": "^11.1.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "puppeteer": "^24.37.4",
        "razorpay": "^2.9.6",
        "reflect-metadata": "0.2.2",
        "rimraf": "6.0.1",
        "rxjs": "7.8.1",
        "slugify": "^1.6.6",
        "ts-morph": "25.0.1",
        "tsx": "^4.19.3",
        "typeorm": "^0.3.20",
        "unique-username-generator": "^1.4.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.2.0",
        "@eslint/js": "9.20.0",
        "@nestjs/cli": "11.0.4",
        "@nestjs/schematics": "11.0.1",
        "@nestjs/testing": "11.0.10",
        "@types/express": "5.0.0",
        "@types/jest": "29.5.14",
        "@types/luxon": "^3.4.2",
        "@types/node": "22.13.4",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "6.0.2",
        "eslint": "9.20.1",
        "eslint-config-prettier": "10.0.1",
        "eslint-plugin-prettier": "5.2.3",
        "globals": "15.15.0",
        "jest": "29.7.0",
        "prettier": "3.5.1",
        "supertest": "7.0.0",
        "ts-jest": "29.2.5",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.7.3",
        "typescript-eslint": "8.24.1",
        "webpack": "5.98.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
